                    TweenService:Create(child, 
                        TweenInfo.new(0.6, Enum.EasingStyle.Quart, Enum.EasingDirection.Out), 
                        {TextTransparency = 0}
                    ):Play()
                elseif child:IsA(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'**********************************************************************','******************************************************************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************'})) and not child:IsA(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'*************************************************************************************','*************************************************************************','*******************************************************************','***************************************************************************************************************','******************************************************************************************************************','**************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************'})) and not child:IsA(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'*************************************************************************************','*************************************************************************','***********************************************************************************','********************************************************************************************************************','******************************************************************************************************************','*************************