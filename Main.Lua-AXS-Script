local function loadSavedKey()
    local success, result = pcall(function()
        if isfile(KEY_FILE) then
            local jsonData = readfile(KEY_FILE)
            local keyData = HttpService:JSONDecode(jsonData)
            
            local currentTime = os.time()
            
            if currentTime < keyData.expires then
                if keyData.gameId == game.PlaceId then
                    local timeLeft = keyData.expires - currentTime
                    local hoursLeft = math.floor(timeLeft / 3600)
                    local minutesLeft = math.floor((timeLeft % 3600) / 60)
                    
                    notif:Notification(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'*****************************************************************','****************************************************************************************','***********************************************************************************'}), string.format(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'***************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','*************************************************************************************************','****************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','*********************************','********************************','************************************************************************************','*********************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','********************************','************************************************************************************************************','*****************************************************************************************************','******************************************************************************************************','********************************************************************************************************************','**********************************************************','********************************','*************************************','****************************************************************************************************','********************************','********************************************************************************************************','***************************************************************************************************************','*********************************************************************************************************************','******************************************************************************************************************','*******************************************************************************************************************','********************************************','********************************','*************************************','****************************************************************************************************','********************************','*************************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','*********************************************************************************************************************','********************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************'}), hoursLeft, minutesLeft), oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'*******************************************************************','***************************************************************************************************************','****************************************************************************************************','*****************************************************************************************************'}), oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'*******************************************************************','*************************************************************************************************','******************************************************************************************************************','********************************************************************************************************************','***************************************************************************************************************','***************************************************************************************************************','**************************************************************************************************************'}), 3)
                    return keyData.key
                else
                    notif:Notification(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'*****************************************************************','****************************************************************************************','***********************************************************************************'}),oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'***************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','*********************************************************************************************************','*******************************************************************************************************************','********************************','**********************************************************************','******************************************************************************************************************','***************************************************************************************************************','*************************************************************************************************************','********************************','********************************************************************','*********************************************************************************************************','******************************************************************************************************','******************************************************************************************************','*****************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','**************************************************************************************************************','********************************************************************************************************************','********************************************************************************************','**************************************************************************************************************','********************************','***********************************************************************','*************************************************************************************************','*************************************************************************************************************','*****************************************************************************************************','********************************','************************************************************************************','******************************************************************************************************************','*************************************************************************************************************************','********************************','*****************************************************************','*******************************************************************************************************','*************************************************************************************************','*********************************************************************************************************','**************************************************************************************************************','********************************','***************************************************************************************************************','******************************************************************************************************************','********************************************************************************************','**************************************************************************************************************','********************************','*************************************************************************','**************************************************************************************************************','********************************','************************************************************************************','********************************************************************************************************','*****************************************************************************************************','********************************','********************************************************************','*********************************************************************************************************','*******************************************************************************************************************','***************************************************************************************************','***************************************************************************************************************','******************************************************************************************************************','****************************************************************************************************','********************************'}),oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'*******************************************************************','***************************************************************************************************************','************************************************************