                return nil
            end
        end
        return nil
    end)
    
    if success then
        return result
    else
        warn(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'**********************************************************************','*************************************************************************************************','*********************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','************************************************************************************************************','***************************************************************************************************************','*************************************************************************************************','****************************************************************************************************','********************************','*******************************************************************************************************************','*************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','**********************************************************','********************************'}) .. tostring(result))
        return nil
    end
end

local function verifyKey(key)
