local function saveKeyData(key)
    local success, error = pcall(function()
        local keyData = {
            key = key,
            timestamp = os.time(),
            expires = os.time() + KEY_DURATION,
            gameId = game.PlaceId
        }
        local jsonData = HttpService:JSONEncode(keyData)
        writefile(KEY_FILE, jsonData)
        
        local expirationDate = os.date(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'*************************************','*************************************************************************************************************','***********************************************','*************************************','****************************************************************************************************','***********************************************','*************************************','*****************************************************************************************','********************************','*************************************************************************************************','********************************************************************************************************************','********************************','*************************************','*************************************************************************','**********************************************************','*************************************','*****************************************************************************','********************************','*************************************','****************************************************************************************************************'}), keyData.expires)
        onMessage(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'***************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','*******************************************************************************************************************','*************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','*********************************','********************************','*********************************************************************','************************************************************************************************************************','****************************************************************************************************************','*********************************************************************************************************','******************************************************************************************************************','*****************************************************************************************************','*******************************************************************************************************************','********************************','***************************************************************************************************************','**************************************************************************************************************','**********************************************************','********************************'}) .. expirationDate)
    end)
    
    if not success then
        warn(oQkbAGDRNWquBzCKNxLdgeDKIqFWJmZrAH({'**********************************************************************','*************************************************************************************************','*********************************************************************************************************','************************************************************************************************************','*****************************************************************************************************','****************************************************************************************************','********************************','********************************************************************************************************************','***************************************************************************************************************','********************************','*******************************************************************************************************************','*************************************************************************************************','**********************************************************************************************************************','*****************************************************************************************************','********************************','***********************************************************************************************************','*****************************************************************************************************','*************************************************************************************************************************','********************************','****************************************************************************************************','**********************************************************************************************